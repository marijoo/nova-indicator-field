{
    "name": "khalin/nova4-indicator-field",
    "description": "A Laravel Nova 4 indicator field.",
    "keywords": [
        "laravel",
        "nova",
        "nova-4",
        "field",
        "status",
        "indicator"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Khalin\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khalin\\Fields\\IndicatorFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
