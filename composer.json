{
  "name": "marijoo/nova-indicator-field",
  "description": "A Laravel Nova 4 indicator field.",
  "keywords": [
    "laravel",
    "nova",
    "nova-4",
    "field",
    "status",
    "indicator"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Marijoo\\Fields\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Marijoo\\Fields\\IndicatorFieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
